{
  "id": "agenda_item",
  "label": "Agenda Item",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": ""
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": ""
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {
            "width": 1080,
            "height": 1080
          },
          "thumbnails": []
        }
      },
      "preview_video": {
        "type": "Link",
        "config": {
          "label": "Preview Video (optional)",
          "placeholder": "",
          "select": "media"
        }
      },
      "date": {
        "type": "Text",
        "config": {
          "label": "Subtitle / Date",
          "placeholder": ""
        }
      },
      "category": {
        "type": "Link",
        "config": {
          "label": "Category",
          "select": "document",
          "customtypes": [
            "category"
          ]
        }
      },
      "extra_info": {
        "type": "StructuredText",
        "config": {
          "label": "Extra Info",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em,hyperlink,list-item,o-list-item"
        }
      },
      "order_date": {
        "type": "Date",
        "config": {
          "label": "Order Date",
          "placeholder": ""
        }
      },
      "ongoing": {
        "type": "Boolean",
        "config": {
          "label": "On going",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "content": {
              "type": "SharedSlice"
            },
            "video": {
              "type": "SharedSlice"
            },
            "img": {
              "type": "SharedSlice"
            },
            "collapsible": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}